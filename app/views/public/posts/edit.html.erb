<h1>投稿編集画面</h1>
<%= form_with model: @post , url: post_path(@post) do |f| %>
  <div class="field">
    <%= f.label :ニックネーム %>
    <%= f.field :name %>
  </div>
  <div class="field">
    <%= f.label :投稿画像 %>
    <%= f.field :post_image %>
  </div>
  <div class="field">
    <%= f.label :自己紹介 %>
    <%= f.field :introduce %>
  </div>
<% end %>
