フォロワー一覧
<% @users.each do |user|%>
  <%= image_tag user.get_profile_image(100,100)%>
  <%= user.name %>
  <%= user.introduce %>
  <% if current_user.following?(@user) %>
    <%= link_to "フォローをやめる", user_relationships_path(@user.id), method: :delete %>
  <% else %>
    <%= link_to "フォローバックする", user_relationships_path(@user.id), method: :post %>
  <% end %>
<% end %>
